    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "\u7802\u6f20\u3067\u3000\u751f\u6d3b\u3059\u308b\u3002\n\u592a\u967d\u306e\u3000\u5149\u3092\u3000\u6d74\u3073\u3066\u3000\u767a\u96fb\u3059\u308c\u3070\n\u30a8\u30b5\u3092\u3000\u98df\u3079\u306a\u304f\u3066\u3082\u3000\u5e73\u6c17\u306a\u306e\u3060\u3002",
