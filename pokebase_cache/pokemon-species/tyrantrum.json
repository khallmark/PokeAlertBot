    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "Nothing could stop this Pok\u00e9mon\n100 million years ago,\nso it behaved like a king.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
