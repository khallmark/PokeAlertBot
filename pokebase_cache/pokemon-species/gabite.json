    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "Ha l\u2019abitudine di collezionare gemme che trova mentre si scava\nla tana, appositamente ampliata.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/8/",
