    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "As it digs to expand its nest, it habitually digs up\ngems that it then hoards in its nest.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
