    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "When you see Leafeon asleep in a patch of\nsunshine, you\u2019ll know it is using photosynthesis\nto produce clean air.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
