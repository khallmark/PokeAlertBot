      }
    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "\u30b5\u30f3\u30b4\u306e\u3000\u679d\u306e\u3000\u3059\u304d\u9593\u306b\u3000\u6f5c\u3063\u3066\u3000\u7720\u308b\u3002\n\u30b7\u30fc\u30c9\u30e9\u306b\u3000\u6c17\u3065\u304b\u305a\u3000\u30b5\u30f3\u30b4\u3092\u3000\u53d6\u308d\u3046\u3068\u3057\u3066\n\u6bd2\u306e\u3000\u30c8\u30b2\u306b\u3000\u523a\u3055\u308c\u3066\u3057\u307e\u3046\u3000\u3053\u3068\u304c\u3000\u3042\u308b\u3002",
