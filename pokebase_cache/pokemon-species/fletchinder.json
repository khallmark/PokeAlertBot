    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "Es speit Funken aus seinem Schnabel und f\u00e4ngt die Beute,\ndie \u00fcberrascht aus dem angesengten Gras hervorspringt.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/6/",
        "name": "de"
