      }
    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/28/",
        "name": "moon"
      },
      "flavor_text": "\u3042\u305f\u307e\u306e\u3000\u3048\u3060\u3092\u3000\u306d\u3089\u3044\u3000\u30d2\u30c9\u30a4\u30c7\u304c\n\u304a\u3044\u304b\u3051\u3066\u304f\u308b\u3068\u3000\u3058\u3076\u3093\u3067\u3000\u3048\u3060\u3092\n\u30dd\u30ad\u30ea\u3068\u3000\u304a\u3063\u3066\u3000\u306b\u3052\u3060\u3059\u3088\u3002",
