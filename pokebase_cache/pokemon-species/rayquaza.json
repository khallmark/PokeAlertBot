    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/24/",
        "name": "y"
      },
      "flavor_text": "\u306f\u308b\u304b\u3000\u4e0a\u7a7a\u306e\u3000\u30aa\u30be\u30f3\u5c64\u306e\u3000\u4e2d\u3092\n\u98db\u3093\u3067\u3044\u308b\u305f\u3081\u3000\u6700\u8fd1\u307e\u3067\n\u59ff\u3092\u3000\u898b\u305f\u3000\u8005\u306f\u3000\u3044\u306a\u304b\u3063\u305f\u3002",
