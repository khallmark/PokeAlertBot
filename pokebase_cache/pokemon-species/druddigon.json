    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "Il utilise ses ailes pour emmagasiner la chaleur du soleil.\nSi la temp\u00e9rature de son corps baisse, il ne peut plus bouger.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/5/",
        "name": "fr"
