    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "\u304b\u3093\u305d\u3046\u3057\u305f\u3000\u304f\u3046\u304d\u3092\u3000\u3053\u306e\u3080\u3002\n\u3059\u3044\u3076\u3093\u3092\u3000\u3061\u3087\u3046\u305b\u3064\u3000\u3059\u308b\u3068\u304d\u306b\n\u3060\u3059\u3000\u202f\u305a\u304c\u3000\u306a\u202f\u3060\u306b\u3000\u202f\u3048\u308b\u3002",
