    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "Smoochum actively runs about, but also falls quite often.\nWhenever the chance arrives, it will look for its reflection to\nmake sure its face hasn\u2019t become dirty.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
