    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "They never allow prey to escape. Their jaws\nare so powerful, they can crush the body of\nan automobile.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
