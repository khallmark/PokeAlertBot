    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "When the horns on its head shine\nin seven colors, it is said to be\nsharing everlasting life.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
