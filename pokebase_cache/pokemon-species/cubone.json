    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/28/",
        "name": "moon"
      },
      "flavor_text": "\u3042\u305f\u307e\u306b\u3000\u304b\u3076\u3063\u305f\u3000\u305a\u304c\u3044\u3053\u3064\u306f\n\u3057\u3093\u3060\u3000\u306f\u306f\u304a\u3084\u3002\u3000\u3057\u306e\u3000\u304b\u306a\u3057\u307f\u3092\n\u306e\u308a\u3053\u3048\u305f\u3068\u304d\u3000\u3057\u3093\u304b\u3059\u308b\u3000\u3068\u3044\u3046\u3002",
