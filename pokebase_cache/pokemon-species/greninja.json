    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "It creates throwing stars out of compressed water.\nWhen it spins them and throws them at high speed,\nthese stars can split metal in two.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
