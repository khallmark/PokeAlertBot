    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "\u00c8 scattante come una saetta. Se corre alla massima velocit\u00e0,\nsi sente un rimbombo di tuoni.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/8/",
        "name": "it"
