    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "When it gets excited, embers rise from its head\nand tail and it gets hot. For some reason, it\nloves sweets.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
