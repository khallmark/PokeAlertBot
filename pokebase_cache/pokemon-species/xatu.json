    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/24/",
        "name": "y"
      },
      "flavor_text": "Si dice che sia cos\u00ec quieto perch\u00e9 sta osservando\npassato e futuro allo stesso tempo.",
      "language": {
