      }
    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/24/",
        "name": "y"
      },
      "flavor_text": "\u6697\u3044\u3000\u6d1e\u7a9f\u306b\u3000\u4f4f\u51e6\u3092\u3000\u4f5c\u308a\n\u92ed\u3044\u3000\u30c4\u30e1\u3092\u3000\u4f7f\u3063\u3066\n\u5b9d\u77f3\u3092\u3000\u6398\u308a\u51fa\u3057\u3066\u306f\u3000\u98df\u3079\u308b\u3002",
      "language": {
