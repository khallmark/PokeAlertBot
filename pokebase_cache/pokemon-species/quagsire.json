    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "Morlord jagt seine Nahrung, indem es im Wasser sein Maul\nweit aufmacht und auf unvorsichtige Beute wartet. Da sich\ndieses Pok\u00e9mon nicht bewegt, wird es auch nicht besonders\nhungrig.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/6/",
        "name": "de"
