    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "Inhaling the gas they belch will make you sleep for\na week. They prefer unsanitary places.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
