    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "When it gets dirty, its leaves can\u2019t be used in\nphotosynthesis, so it always keeps itself clean.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
