    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/23/",
        "name": "x"
      },
      "flavor_text": "\u4f55\u4e16\u4ee3\u306b\u3082\u3000\u6e21\u3063\u3066\n\u30cf\u30d6\u30cd\u30fc\u30af\u3068\u3000\u6226\u3063\u3066\u304d\u305f\u3002\n\u92ed\u3044\u3000\u30c4\u30e1\u304c\u3000\u6700\u5927\u306e\u3000\u6b66\u5668\u3002",
