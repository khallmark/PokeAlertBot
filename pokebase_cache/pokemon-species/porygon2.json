    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "Porygon2 was created by humans using the power of science.\nThe man-made Pok\u00e9mon has been endowed with artificial\nintelligence that enables it to learn new gestures and emotions\non its own.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
