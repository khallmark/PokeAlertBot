    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "Doduo\u2019s two heads contain completely identical brains. A\nscientific study reported that on rare occasions, there will be\nexamples of this Pok\u00e9mon possessing different sets of brains.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
