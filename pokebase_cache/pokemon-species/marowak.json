    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "Marowak is the evolved form of a Cubone that has overcome\nits sadness at the loss of its mother and grown tough. This\nPok\u00e9mon\u2019s tempered and hardened spirit is not easily broken.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
