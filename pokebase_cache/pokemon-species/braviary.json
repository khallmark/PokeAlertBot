    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "Un valiente guerrero del cielo que no duda en luchar por sus\ncompa\u00f1eros por graves que sean sus heridas.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/7/",
        "name": "es"
