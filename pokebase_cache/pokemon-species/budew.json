    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "Bei Sonnenlicht \u00f6ffnet es seine Knospe und gibt Pollen ab.\nEs lebt in der N\u00e4he von sauberem Wasser.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/6/",
        "name": "de"
