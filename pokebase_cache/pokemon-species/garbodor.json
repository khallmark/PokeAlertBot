    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "It clenches opponents with its left arm and finishes\nthem off with foul-smelling poison gas belched from\nits mouth.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
