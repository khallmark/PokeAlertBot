    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "Apresa a su rival con el brazo izquierdo y le da el golpe de\ngracia exhalando por la boca un apestoso gas venenoso.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/7/",
        "name": "es"
