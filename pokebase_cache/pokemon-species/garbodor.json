    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "Serra l\u2019avversario con l\u2019arto sinistro e gli d\u00e0 il colpo di grazia\nesalando dalla bocca un fetido gas velenoso.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/8/",
        "name": "it"
