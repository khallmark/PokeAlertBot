    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/23/",
        "name": "x"
      },
      "flavor_text": "\u666e\u6bb5\u306f\u3000\u5c71\u5965\u3067\u3000\u66ae\u3089\u3057\u3066\u3044\u308b\u304c\n\u304a\u8179\u304c\u3000\u3059\u304f\u3068\u3000\u3075\u3082\u3068\u306b\u3000\u73fe\u308f\u308c\n\u7dda\u8def\u3084\u3000\u8eca\u3092\u3000\u98df\u3079\u3066\u3057\u307e\u3046\u3002",
