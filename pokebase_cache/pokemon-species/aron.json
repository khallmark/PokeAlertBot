    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/24/",
        "name": "y"
      },
      "flavor_text": "\u304b\u3089\u3060\u3092\u3000\u304a\u304a\u3046\u3000\u306f\u304c\u306d\u306e\u3000\u3088\u308d\u3044\u306f\n\u3057\u3093\u304b\u3059\u308b\u3068\u304d\u3000\u30dd\u30ed\u30ea\u3068\u3000\u306f\u304c\u308c\u304a\u3061\n\u3042\u305f\u3089\u3057\u3044\u3000\u3088\u308d\u3044\u304c\u3000\u3064\u304f\u3089\u308c\u308b\u3002",
