    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "\uc124\uc0b0\uc5d0 \uc11c\uc2dd\ud558\ub294 \ud3ec\ucf13\ubaac\uc774\ub2e4.\n\uba3c \uc61b\ub0a0 \ube59\ud558\uae30 \ub54c\n\ub0a8\ucabd \ub545\uc73c\ub85c \uc774\ub3d9\ud574 \uc654\ub2e4.",
