    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "Sometimes the magnetism emitted by Magnezone is\ntoo strong, making them attract each other so they\ncannot move.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
