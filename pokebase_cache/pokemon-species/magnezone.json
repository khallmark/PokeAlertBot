    },
    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "A volte si attraggono l\u2019un l\u2019altro con la loro potentissima\ncarica magnetica e poi non riescono pi\u00f9 a muoversi.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/8/",
        "name": "it"
