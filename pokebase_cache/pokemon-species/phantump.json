    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/26/",
        "name": "alpha-sapphire"
      },
      "flavor_text": "\u3082\u308a\u3067\u3000\u3055\u307e\u3088\u3044\u3000\u3057\u3093\u3060\u3000\u3053\u3069\u3082\u306e\n\u305f\u307e\u3057\u3044\u304c\u3000\u304d\u308a\u304b\u3076\u306b\u3000\u3084\u3069\u308a\n\u30dd\u30b1\u30e2\u30f3\u306b\u306a\u3063\u305f\u3068\u3000\u3044\u308f\u308c\u3066\u3044\u308b\u3002",
