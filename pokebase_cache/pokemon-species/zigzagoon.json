    {
      "version": {
        "url": "https://pokeapi.co/api/v2/version/25/",
        "name": "omega-ruby"
      },
      "flavor_text": "Zigzagoon restlessly wanders everywhere at all times.\nThis Pok\u00e9mon does so because it is very curious. It becomes\ninterested in anything that it happens to see.",
      "language": {
        "url": "https://pokeapi.co/api/v2/language/9/",
        "name": "en"
